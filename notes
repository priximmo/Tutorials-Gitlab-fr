stages:
  - yamllint_testing

yamllint_testing:
  except:
    refs:
      - main
  only:
    - merge_requests
  stage: yamllint_testing
  script:
    - docker run --rm -v ${PWD}:/tmp/ kubeval-tools:v2.6 yamllint -c /tmp/.yamllint.rules --no-warnings /tmp/;


extends: default

rules:
  line-length:
    max: 80
    level: warning
  new-line-at-end-of-file: disable
  indentation:
    spaces: consistent
    indent-sequences: whatever
  commas:
    max-spaces-before: 0
    min-spaces-after: 0
    max-spaces-after: 1

