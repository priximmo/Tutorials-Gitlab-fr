repository check trigger


gitlab-runner register \
    --non-interactive \
    --run-untagged \
    --docker-privileged \
    --url https://git.example.com/ \
    --registration-token XXXXX \
    --executor docker \
    --docker-image docker:latest \
    --description gitlab-runner1-dev \
    --tag-list "docker,privileged" \
    --locked="false"
